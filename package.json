    "@types/debug": "^4.1.8",
    "@types/jest": "^29.5.4",
    "@types/lodash.debounce": "^4.0.7",
    "@types/lodash.groupby": "^4.6.7",
    "@types/lodash.keyby": "^4.6.7",
    "@types/lodash.maxby": "^4.6.7",
    "@types/lodash.sortby": "^4.7.7",
    "@types/lodash.throttle": "^4.1.7",
    "@types/node": "^18.17.13",
    "@types/prop-types": "^15.7.5",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@types/sanitize-html": "^2.9.0",
    "@uifabric/icons": "^7.9.5",
    "almin": "^0.19.0",
    "almin-logger": "^6.2.4",
    "almin-react-container": "^0.7.4",
    "classnames": "^2.3.2",
    "combokeys": "^3.0.1",
    "date-fns": "^1.30.1",
    "ddd-base": "^0.9.1",
    "debug": "^4.3.4",
    "he": "^1.2.0",
    "localforage": "^1.10.0",
    "localforage-memoryStorageDriver": "^0.9.2",
    "lodash.debounce": "^4.0.8",
    "lodash.groupby": "^4.6.0",
    "lodash.keyby": "^4.6.0",
    "lodash.maxby": "^4.6.0",
    "lodash.sortby": "^4.7.0",
    "lodash.throttle": "^4.1.1",
    "map-like": "^2.0.0",
    "most-visible": "^1.5.0",
