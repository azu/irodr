    "@types/prop-types": "^15.5.2",
    "@types/react": "^16.0.16",
    "@types/react-dom": "^16.0.2",
    "almin": "^0.14.0",
    "almin-logger": "^6.1.0",
    "almin-react-container": "^0.4.0",
    "classnames": "^2.2.5",
    "client-oauth2": "^4.1.0",
    "combokeys": "^3.0.0",
    "date-fns": "^1.29.0",
    "ddd-base": "^0.3.0",
    "debug": "^3.1.0",
    "he": "^1.1.1",
    "localforage": "^1.5.2",
    "localforage-memoryStorageDriver": "^0.9.2",
    "lodash.debounce": "^4.0.8",
    "lodash.groupby": "^4.6.0",
    "lodash.keyby": "^4.6.0",
    "lodash.maxby": "^4.6.0",
    "lodash.sortby": "^4.7.0",
    "lodash.throttle": "^4.1.1",
    "map-like": "^2.0.0",
    "most-visible": "^1.0.1",
    "office-ui-fabric-react": "^5.11.0",
    "popsicle": "^9.1.0",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-icons": "^2.2.7",
    "react-render-html": "^0.5.2",
    "ulid": "^1.0.2"
  },
  "scripts": {
    "precommit": "lint-staged",
    "postcommit": "git reset",
    "prettier": "prettier --write '{src,resources}/**/*.{css,js,ts,tsx}'",
    "start": "PORT=13245 react-scripts-ts start",
    "build": "SSR=true react-scripts-ts build && react-snap",
    "test": "react-scripts-ts test",
    "eject": "react-scripts-ts eject",
    "netlify": "dotenv -e .env.netlify react-scripts-ts build",
    "pwmerics": "pwmetrics --config ./pwmetrics-config.js",
    "pwmerics:ci": "CI=true NODE_ENV=production pwmetrics --config ./pwmetrics-config.js"
  },
  "lint-staged": {
    "*.{js,ts,tsx,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "proxy": {
    "/cors": {
      "target": "https://jp.inoreader.com",
      "pathRewrite": {
        "^/cors": ""
      },
      "changeOrigin": true
    },
    "/api": {
      "target": "https://jp.inoreader.com/reader",
      "changeOrigin": true
    }
  },
  "devDependencies": {
    "@alrra/travis-scripts": "^3.0.1",
    "dotenv-cli": "^1.4.0",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "lint-staged": "^4.3.0",
    "prettier": "^1.7.4",
    "pwmetrics": "^3.1.2",
    "react-scripts-ts": "^2.8.0",
    "react-snap": "^0.10.0"
  }
}
