{
  "name": "irodr",
  "version": "1.0.0",
  "private": true,
  "description": "inoreader RSS reader client on browser",
  "homepage": "https://irodr.netlify.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/azu/irodr"
  },
  "license": "MIT",
  "author": "azu",
  "scripts": {
    "build": "react-scripts build && react-snap",
    "eject": "react-scripts eject",
    "netlify": "dotenv -e .env.netlify react-scripts build",
    "pwmerics": "pwmetrics --config ./pwmetrics-config.js",
    "pwmerics:ci": "CI=true NODE_ENV=production pwmetrics --config ./pwmetrics-config.js",
    "start": "PORT=13245 react-scripts start",
    "test": "react-scripts test",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,css}\"",
    "prepare": "git config --local core.hooksPath .githooks"
  },
  "husky": {
    "hooks": {
      "post-commit": "git reset",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@immutable-array/prototype": "^1.0.4",
    "@types/classnames": "^2.3.0",
    "@types/combokeys": "^2.4.6",
    "@types/debug": "^4.1.7",
    "@types/jest": "^23.3.14",
    "@types/lodash.debounce": "^4.0.6",
    "@types/lodash.groupby": "^4.6.6",
    "@types/lodash.keyby": "^4.6.6",
    "@types/lodash.maxby": "^4.6.6",
    "@types/lodash.sortby": "^4.7.6",
    "@types/lodash.throttle": "^4.1.6",
    "@types/node": "^12.20.16",
    "@types/prop-types": "^15.7.4",
    "@types/react": "^16.14.11",
    "@types/react-dom": "^16.9.14",
    "@types/sanitize-html": "^1.27.2",
    "@uifabric/icons": "^5.8.0",
    "almin": "^0.19.0",
    "almin-logger": "^6.2.4",
    "almin-react-container": "^0.7.4",
    "classnames": "^2.3.1",
    "client-oauth2": "^4.3.3",
    "combokeys": "^3.0.1",
    "date-fns": "^1.30.1",
    "ddd-base": "^0.9.1",
    "debug": "^4.3.2",
    "he": "^1.2.0",
    "intersection-observer": "^0.12.0",
    "localforage": "^1.9.0",
    "localforage-memoryStorageDriver": "^0.9.2",
    "lodash.debounce": "^4.0.8",
    "lodash.groupby": "^4.6.0",
    "lodash.keyby": "^4.6.0",
    "lodash.maxby": "^4.6.0",
    "lodash.sortby": "^4.7.0",
    "lodash.throttle": "^4.1.1",
    "map-like": "^2.0.0",
    "most-visible": "^1.5.0",
    "office-ui-fabric-react": "^5.135.6",
    "popsicle": "^9.2.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-icons": "^2.2.7",
    "react-render-html": "^0.6.0",
    "requestidlecallback": "^0.3.0",
    "sanitize-html": "^1.27.5",
    "ulid": "^2.3.0",
    "whatwg-fetch": "^3.6.2"
  },
  "devDependencies": {
    "@alrra/travis-scripts": "^3.0.2",
    "@types/http-proxy-middleware": "^0.19.3",
    "dotenv-cli": "^3.2.0",
    "http-proxy-middleware": "^0.21.0",
    "lint-staged": "^11.1.1",
    "prettier": "^2.3.0",
    "pwmetrics": "^4.2.3",
    "react-scripts": "^3.4.4",
    "react-snap": "^1.23.0",
    "typescript": "^3.7.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "singleQuote": false,
    "printWidth": 120,
    "tabWidth": 4,
    "trailingComma": "none"
  }
}
